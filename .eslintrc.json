{
  "extends": ["eslint:recommended"],
  "parserOptions": {
	"sourceType": "module",
	"ecmaVersion": 8
  },
  "env": {
	"node": true,
	"jest": true
  },
  "rules": {
	"semi": 0,
	"vars-on-top": 0,
	"spaced-comment": 0,
	"prefer-template": 0,
	"consistent-return": 0,
	"comma-dangle": 0,
	"max-len": 0,
	"arrow-body-style": 0,
	"new-cap": 0,
	"quotes": 0,
	"quote-props": 0,
	"prefer-arrow-callback": 0,
	"func-names": 0,
	"padded-blocks": 0,
	"keyword-spacing": 0,
	"no-use-before-define": 0,
	"no-return-assign": 0,
	"no-case-declarations": 0,
	"no-cond-assign": 0,
	"no-console": 0,
	"no-useless-escape": 1,
	"no-var": 0,
	"no-trailing-spaces": 0,
	"no-unused-expressions": 0,
	"no-unused-vars": 1,
	"no-inner-declarations": 0,
	"space-before-function-paren": 0,
	"global-require": 0
  }
}
