{
  "name": "postcss-comment-2",
  "version": "1.0.1",
  "description": "Allow postcss to support inline comments. Works with PostCSS 6",
  "main": "index.js",
  "scripts": {
	  "test": "jest -i --colors --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nightwolfz/postcss-comment.git"
  },
  "keywords": [
    "postcss",
    "postcss-runner",
    "postcss-comment",
    "postcss-comment-2",
    "inline",
    "comment"
  ],
  "author": "nightwolfz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nightwolfz/postcss-comment/issues"
  },
  "homepage": "https://github.com/nightwolfz/postcss-comment#readme",
  "dependencies": {
    "postcss": "^6.0.11"
  },
  "devDependencies": {
    "eslint": "^4.6.1",
    "jest": "^21.0.2"
  },
  "jest": {
    "automock": false,
    "bail": true,
    "verbose": true,
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/example/"
    ],
    "testMatch": [
      "**/test/(*.)(spec|test).js"
    ]
  }
}
